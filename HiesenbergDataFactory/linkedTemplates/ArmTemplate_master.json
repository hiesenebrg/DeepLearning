{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "HiesenbergDataFactory"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureDataLakeStorage2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage2'"
		},
		"AzureDataLakeStorage3_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage3'"
		},
		"countryLookup_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'countryLookup'"
		},
		"coviddeath_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'coviddeath'"
		},
		"hospitalsink_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'hospitalsink'"
		},
		"testingsink_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'testingsink'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://hiesenberghtorage.dfs.core.windows.net/"
		},
		"AzureDataLakeStorage2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://hiesenberghtorage.dfs.core.windows.net/"
		},
		"AzureDataLakeStorage3_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://hiesenberghtorage.dfs.core.windows.net/"
		},
		"HttpServer1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://opendata.ecdc.europa.eu/covid19/testing/csv"
		},
		"HttpServerforhospital_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://opendata.ecdc.europa.eu/covid19/hospitalicuadmissionrates/csv"
		},
		"countryLookup_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://hiesenberghtorage.dfs.core.windows.net/"
		},
		"coviddeath_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://hiesenberghtorage.dfs.core.windows.net/"
		},
		"hospitalsink_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://hiesenberghtorage.dfs.core.windows.net/"
		},
		"httpforcoviddeath_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://opendata.ecdc.europa.eu/covid19/nationalcasedeath/csv"
		},
		"testingsink_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://hiesenberghtorage.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "HiesenbergDataFactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureDataLakeStorage2_accountKey": {
						"value": "[parameters('AzureDataLakeStorage2_accountKey')]"
					},
					"AzureDataLakeStorage3_accountKey": {
						"value": "[parameters('AzureDataLakeStorage3_accountKey')]"
					},
					"countryLookup_accountKey": {
						"value": "[parameters('countryLookup_accountKey')]"
					},
					"coviddeath_accountKey": {
						"value": "[parameters('coviddeath_accountKey')]"
					},
					"hospitalsink_accountKey": {
						"value": "[parameters('hospitalsink_accountKey')]"
					},
					"testingsink_accountKey": {
						"value": "[parameters('testingsink_accountKey')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage2_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage2_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage3_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage3_properties_typeProperties_url')]"
					},
					"HttpServer1_properties_typeProperties_url": {
						"value": "[parameters('HttpServer1_properties_typeProperties_url')]"
					},
					"HttpServerforhospital_properties_typeProperties_url": {
						"value": "[parameters('HttpServerforhospital_properties_typeProperties_url')]"
					},
					"countryLookup_properties_typeProperties_url": {
						"value": "[parameters('countryLookup_properties_typeProperties_url')]"
					},
					"coviddeath_properties_typeProperties_url": {
						"value": "[parameters('coviddeath_properties_typeProperties_url')]"
					},
					"hospitalsink_properties_typeProperties_url": {
						"value": "[parameters('hospitalsink_properties_typeProperties_url')]"
					},
					"httpforcoviddeath_properties_typeProperties_url": {
						"value": "[parameters('httpforcoviddeath_properties_typeProperties_url')]"
					},
					"testingsink_properties_typeProperties_url": {
						"value": "[parameters('testingsink_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "HiesenbergDataFactory_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'HiesenbergDataFactory_ArmTemplate_0')]"
			]
		}
	]
}